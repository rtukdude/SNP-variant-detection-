    "#!/usr/bin/env python3\n",
    "# -*- coding: utf-8 -*-\n",
    "\"\"\"\n",
    "Created on Fri Dec 20 20:10:22 2019\n",
    "\n",
    "@author: raoulrosenthal\n",
    "\"\"\"\n",
    "\n",
    "# -*- coding: utf-8 -*-\n",
    "\"\"\"Amino acid mutation.ipynb\n",
    "\n",
    "Automatically generated by Colaboratory.\n",
    "\n",
    "Original file is located at\n",
    "    https://colab.research.google.com/drive/1dQGGuQUbD7YdX1oVSAoi_lOYuJcaKueS\n",
    "\"\"\"\n",
    "\n",
    "# Run this part to upload files from local\n",
    "\n",
    "\n",
    "from Bio import SeqIO\n",
    "from Bio import Align\n",
    "from Bio.Align import AlignInfo\n",
    "from Bio import AlignIO\n",
    "from Bio.Seq import Seq\n",
    "from Bio.Alphabet import IUPAC\n",
    "from Bio import pairwise2\n",
    "from Bio.Alphabet import generic_dna\n",
    "import glob\n",
    "import os\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def string_replace(string, new_character, position):\n",
    "    return(string[0:position] + str(new_character) + string[position+1:])\n",
    "\n",
    "def import_sequences(file_path, type_='fastq'): #imports sequences from file \n",
    "    Tracefiles = glob.glob(file_path)\n",
    "    list_of_sequences=[]\n",
    "    for file in Tracefiles:\n",
    "        for sequence in SeqIO.parse(file, type_):\n",
    "            list_of_sequences.append(sequence)\n",
    "    return(list_of_sequences)\n",
    "\n",
    "def reverse_sequences(list_of_sequences, string_to_reverse): #a function that returns the reverse complement of sequences that have a given string in their file name\n",
    "    for i in range(len(list_of_sequences)):\n",
    "        sequence = list_of_sequences[i].seq\n",
    "        if(len(sequence)) > 25:\n",
    "            if string_to_reverse in list_of_sequences[i].id:            # find all reverse traces\n",
    "                list_of_sequences[i].seq = sequence.reverse_complement()\n",
    "    return(list_of_sequences)\n",
    "\n",
    "def load_reference(file_path, type_ = \"genbank\", sequence_number = 0): #loads reference sequence\n",
    "    reference_plasmid = [i for i in SeqIO.parse(file_path, type_)][sequence_number]\n",
    "    reference_plasmid = reference_plasmid\n",
    "    return(reference_plasmid.seq)\n",
    "\n",
    "def find_SNPs(list_of_sequences, reference_sequence, alignment_threshold = -150): # finds all the SNPs in a given variant (both forward and reverse sequence) in the overlap region the SNP with a higher phred score is taken the list of sequences \n",
    "    list_of_SNPS = [] \n",
    "    not_aligned = 0 \n",
    "    for i in range(0,len(list_of_sequences),2):\n",
    "#        counter += 1\n",
    "        read_1 = list_of_sequences[i].seq[:] # read in forward sequence\n",
    "        read_2 = list_of_sequences[i+1].seq[:] # read in reverse sequence\n",
    "        try: # align both reads to reference sequence \n",
    "            alignment1 = pairwise2.align.globalms(reference_gene,read_1,2,-0.5,-4,-2, one_alignment_only =1)[0]\n",
    "            alignment2 = pairwise2.align.globalms(reference_gene,read_2,2,-0.5,-4,-2, one_alignment_only =1)[0]\n",
    "\n",
    "        except IndexError:\n",
    "            not_aligned += 1\n",
    "            print('sequence', i, ' is not well aliged')\n",
    "            continue\n",
    "        mutation_position  = []\n",
    "        reference_base = []\n",
    "        mutated_base = [] \n",
    "        if alignment1[-3]<alignment_threshold or alignment2[-3]<alignment_threshold: # check for good quality alignments i.e. score > 50\n",
    "            not_aligned += 1\n",
    "            print('sequence', i, ' is not well aliged')\n",
    "        else:\n",
    "            forward_seq = alignment1[1]\n",
    "            reverse_seq = alignment2[1]\n",
    "            assembeled_gene = str()\n",
    "    #        print(len(forward_seq), len(reverse_seq))\n",
    "            for nucleotide in range(len(reference_gene)): # index bases according to reference sequence\n",
    "                if (forward_seq[nucleotide] != '-' \n",
    "                    and forward_seq[nucleotide] != reference_gene[nucleotide] \n",
    "                    and reverse_seq[nucleotide] == '-'): # only compares mutations where there is a single forward read\n",
    "\n",
    "                    mutation_position.append(nucleotide)\n",
    "                    reference_base.append(reference_gene[nucleotide])\n",
    "                    mutated_base.append(forward_seq[nucleotide])\n",
    "\n",
    "                elif (reverse_seq[nucleotide] != '-' \n",
    "                      and reverse_seq[nucleotide] != reference_gene[nucleotide] \n",
    "                      and forward_seq[nucleotide] == '-'): # only compares mutations where there is a single reverse read\n",
    "\n",
    "                    mutation_position.append(nucleotide)\n",
    "                    reference_base.append(reference_gene[nucleotide])\n",
    "                    mutated_base.append(reverse_seq[nucleotide])\n",
    "\n",
    "                elif (reverse_seq[nucleotide] != '-' and forward_seq[nucleotide] != '-'): # compares mutations where there is double coverage\n",
    "\n",
    "\n",
    "                    if (reverse_seq[nucleotide] != reference_gene[nucleotide] \n",
    "                        or forward_seq[nucleotide] != reference_gene[nucleotide]): # if only one read is mismatched with the ref, take strand with better phred score \n",
    "\n",
    "                        aligned_forward_seq = forward_seq[0:nucleotide].strip('-')\n",
    "                        aligned_reverse_seq = reverse_seq[0:nucleotide].strip('-')\n",
    "                        forward_position = len(aligned_forward_seq)\n",
    "                        reverse_position = len(aligned_reverse_seq)\n",
    "                        phred_score_F = recordlist[i].letter_annotations['phred_quality'][forward_position-aligned_forward_seq.count('-')]\n",
    "                        phred_score_R = recordlist[i+1].letter_annotations['phred_quality'][reverse_position-aligned_reverse_seq.count('-')]\n",
    "\n",
    "                        if phred_score_F >= phred_score_R:\n",
    "                            if forward_seq[nucleotide] != reference_gene[nucleotide]:\n",
    "                                mutation_position.append(nucleotide)\n",
    "                                reference_base.append(reference_gene[nucleotide])\n",
    "                                mutated_base.append(forward_seq[nucleotide])\n",
    "                            else:\n",
    "                                continue\n",
    "                        if phred_score_F < phred_score_R:\n",
    "                            if reverse_seq[nucleotide] != reference_gene[nucleotide]:\n",
    "                                mutation_position.append(nucleotide)\n",
    "                                reference_base.append(reference_gene[nucleotide])\n",
    "                                mutated_base.append(reverse_seq[nucleotide])\n",
    "                            else: \n",
    "                                continue\n",
    "                    elif (reverse_seq[nucleotide] != reference_gene[nucleotide] \n",
    "                          and forward_seq[nucleotide] != reference_gene[nucleotide]): # if strands are mismatched with the reference, it does not matter with strand is taken to annotate the mutation \n",
    "\n",
    "                        if reverse_seq[nucleotide] == forward_seq[nucleotide]:\n",
    "                            mutation_position.append(nucleotide)\n",
    "                            reference_base.append(reference_gene[nucleotide])\n",
    "                            mutated_base.append(forward_seq[nucleotide])\n",
    "\n",
    "            list_of_SNPS.append([mutation_position, reference_base, mutated_base])\n",
    "    \n",
    "    print('\\n', not_aligned,'out of', len(list_of_sequences)//2, ' sequences could not be aligned well')\n",
    "    return(list_of_SNPS)\n",
    "\n",
    "def classify_mutations(mutations): # counts mutsations by transition type\n",
    "    AC, AG, AT, CA, CG, CT, GA, GC, GT, TA, TC, TG = [0]*12 \n",
    "    all_A = 0\n",
    "    all_C = 0\n",
    "    all_G = 0\n",
    "    all_T = 0\n",
    "    all_GC = 0\n",
    "    all_AT = 0\n",
    "    counter = 0\n",
    "    for i in mutations:\n",
    "        for j in range(len(i[1])):\n",
    "            r = i[1][j]\n",
    "            m = i[2][j]\n",
    "            if r == 'A' and m =='C':\n",
    "                AC += 1\n",
    "            if r == 'A' and m =='G':\n",
    "                AG += 1\n",
    "            if r == 'A' and m =='T':\n",
    "                AT += 1\n",
    "            if r == 'C' and m =='A':\n",
    "                CA += 1\n",
    "            if r == 'C' and m =='G':\n",
    "                CG += 1\n",
    "            if r == 'C' and m =='T':\n",
    "                CT += 1\n",
    "            if r == 'G' and m =='A':\n",
    "                GA += 1\n",
    "            if r == 'G' and m =='C':\n",
    "                GC += 1\n",
    "            if r == 'G' and m =='T':\n",
    "                GT += 1\n",
    "            if r == 'T' and m =='A':\n",
    "                TA += 1\n",
    "            if r == 'T' and m =='C':\n",
    "                TC += 1\n",
    "            if r == 'T' and m =='G':\n",
    "                TG += 1\n",
    "            counter +=1\n",
    "    all_A = AC+AG+AT\n",
    "    all_C = CA+CG+CT\n",
    "    all_G = GA+GC+GT\n",
    "    all_T = TA+TC+TG\n",
    "    all_GC = all_C + all_G\n",
    "    all_AT = all_A + all_T\n",
    "    print('total # mutations', counter, '\\n',AC, AG, AT, CA, CG, CT, GA, GC, GT, TA, TC, TG, '\\n', \n",
    "          'total A:', all_A, (all_A)/ counter, '\\n', \n",
    "          'total C:', all_C, (all_C)/ counter, '\\n',\n",
    "          'total G:', all_G, (all_G)/ counter, '\\n', \n",
    "          'total T:', all_T, (all_T)/ counter, '\\n',\n",
    "          'total GC:', all_GC, all_GC/ counter, '\\n',\n",
    "          'total AT:', all_AT, all_AT/ counter, '\\n',\n",
    "         )\n",
    "    return(all_AT, all_GC, all_A, all_C, all_G, all_T)\n",
    "\n",
    "def plot_mutation_distribution(list_of_SNPS): # plots a histogram of the number of mutations per sequence\n",
    "    mutation_number = []\n",
    "    for SNP in list_of_SNPS:\n",
    "        mutation_number.append(len(SNP[0]))\n",
    "    fig, ax = plt.subplots(1,1, figsize=(6,6))\n",
    "    ax.hist(mutation_number, bins=max(mutation_number)*2+1, density = False)\n",
    "    ax.set(title='mutation distribution ', xlabel= '# of mutations per sequence', ylabel='# of sequences')\n",
    "    return(plt.show())\n",
    "\n",
    "def count_amino_acid_transitions(amino_acid_mutations, amino_acid): # counts the number of mutations that transitioned the sequence into a given amino acid\n",
    "    count_amino_acid = 0 \n",
    "    for mutation in amino_acid_mutations:\n",
    "        count_amino_acid += mutation[-1].count(str(amino_acid))\n",
    "    return(count_amino_acid)\n",
    "\n",
    "def find_AA_mutations_from_SNPs(list_of_SNPS, reference_gene): # translates reference and find amino acid mutations that correspond to SNPs\n",
    "    if type(reference_gene) != type(str()):\n",
    "        return(print('reference_gene must be a string'))\n",
    "    else:\n",
    "        reference_gene = Seq(str(reference_gene), generic_dna) \n",
    "        reference_gene_T = str()\n",
    "        mutations_AA = [] \n",
    "        for SNPS_in_sequence in list_of_SNPS:\n",
    "            original_AA = []\n",
    "            mutated_AA = []\n",
    "            for SNP in range(len(SNPS_in_sequence[0])):\n",
    "                original_AA.append(reference_gene.translate()[(SNPS_in_sequence[0][SNP])//3])\n",
    "                reference_gene = string_replace(reference_gene, SNPS_in_sequence[2][SNP], SNPS_in_sequence[0][SNP])\n",
    "                mutated_AA.append(reference_gene.translate()[(SNPS_in_sequence[0][SNP])//3])\n",
    "            mutations_AA.append([original_AA, mutated_AA])\n",
    "        return(mutations_AA)"
   ]
